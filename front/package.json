{
  "name": "nuvia",
  "private": true,
  "version": "0.0.0",
  "description": "Nuvia",
  "devDependencies": {
    "angular-mocks": "^1.5.8 <1.6.0",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.5.2",
    "jscs": "^3.0.7",
    "jshint": "^2.9.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "ngtemplate-loader": "^1.3.1",
    "protractor": "^3.2.2",
    "style-loader": "^0.13.1",
    "svg-loader": "0.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "archiver": "^2.1.0",
    "nexus-deployer": "^0.1.8"
  },
  "scripts": {
    "predist-dev": "npm install --no-optional",
    "dist-dev": "webpack -d --hash",
    "predist": "npm install --no-optional",
    "dist": "webpack -p --hash",
    "prestart": "npm install --no-optional",
    "start": "webpack-dev-server -d --progress --colors --port 8888 --content-base dist",
    "pretest-headless": "npm install --no-optional",
    "test-headless": "jshint -c .jshintrc src --verbose & jscs src -c .jscsrc & karma start karma.conf.js --browsers PhantomJS2",
    "pretest": "npm install --no-optional",
    "test": "jshint -c .jshintrc src --verbose & jscs src -c .jscsrc & karma start karma.conf.js",
    "test-watch": "jshint -c .jshintrc src --verbose & jscs src -c .jscsrc & karma start karma.conf.js --auto-watch --no-single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "clean": "rimraf dist & rimraf build",
    "prepackage": "npm run dist & rimraf build & mkdir build",
    "package": "node archiver.conf.js",
    "predeploy": "npm run package",
    "deploy": "node deploy.conf.js"
  },
  "dependencies": {
    "angular": "^1.5.8 <1.6.0",
    "angular-animate": "^1.5.8 <1.6.0",
    "angular-bootstrap-datetimepicker": "^1.1.4",
    "angular-dynamic-locale": "^0.1.32",
    "angular-i18n": "^1.5.8 <1.6.0",
    "angular-loader": "^1.5.8 <1.6.0",
    "angular-resource": "^1.5.8 <1.6.0",
    "angular-route": "^1.5.8 <1.6.0",
    "angular-sanitize": "^1.5.8 <1.6.0",
    "angular-translate": "^2.13.0",
    "angular-ui-bootstrap": "^1.1.0",
    "array.prototype.find": "^2.0.4",
    "bootstrap": "^3.3.7",
    "cmelo-angular-sticky": "^1.0.2",
    "ng-file-upload": "^12.2.13"
  }
}
